<%= simple_form_for(@message) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :sender_id %>
    <%= f.input :receiver_id %>
    <%= f.input :message %>
    <%= f.input :read %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
